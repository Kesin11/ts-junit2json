{
  "name": "junit2json",
  "version": "3.0.0",
  "description": "JUnit XML format to JSON with TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "bin": {
    "junit2json": "dist/cli.js"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "release:prepublish": "npm run clean && npm run build && npm version prerelease && npm publish --tag=beta",
    "release:prepare": "npx shipjs prepare",
    "release:trigger": "npx shipjs trigger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kesin11/ts-junit2json.git"
  },
  "keywords": [
    "junit",
    "parser"
  ],
  "author": "kesin1202000@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kesin11/ts-junit2json/issues"
  },
  "homepage": "https://github.com/Kesin11/ts-junit2json#readme",
  "dependencies": {
    "@types/xml2js": "0.4.9",
    "xml2js": "0.4.23",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "16.11.21",
    "@types/yargs": "17.0.8",
    "jest": "27.4.7",
    "jest-junit": "13.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "jest-junit": {
    "suiteName": "ts-junit2json",
    "outputDirectory": "junit",
    "uniqueOutputName": "true",
    "includeConsoleOutput": "true",
    "usePathForSuiteName": "true"
  }
}
