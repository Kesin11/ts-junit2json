{
  "name": "junit2json",
  "version": "3.0.4",
  "description": "JUnit XML format to JSON with TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE"
  ],
  "bin": {
    "junit2json": "dist/cli.js"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "release:prepublish": "npm run clean && npm run build && npm version prerelease && npm publish --tag=beta",
    "release": "earthly --secret NODE_AUTH_TOKEN --secret GITHUB_TOKEN +publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kesin11/ts-junit2json.git"
  },
  "keywords": [
    "junit",
    "parser"
  ],
  "author": "kesin1202000@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kesin11/ts-junit2json/issues"
  },
  "homepage": "https://github.com/Kesin11/ts-junit2json#readme",
  "dependencies": {
    "@types/xml2js": "0.4.11",
    "xml2js": "0.4.23",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@types/yargs": "17.0.22",
    "jest": "29.4.2",
    "jest-junit": "15.0.0",
    "rimraf": "4.1.2",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "jest-junit": {
    "suiteName": "ts-junit2json",
    "outputDirectory": "junit",
    "uniqueOutputName": "true",
    "includeConsoleOutput": "true",
    "usePathForSuiteName": "true"
  }
}
